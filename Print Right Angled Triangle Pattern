t = int(input())
c = 1
for _ in range(t):
    k = int(input())
    print("Case #",end= '')
    print(c,end='')
    print(":")
    for i in range(1, k + 1):
        for j in range(1, k + 1):
            if(j <= k - i):
                print(' ', end = '')
            else:
                print('*', end = '')
        print()
    c = c+1
